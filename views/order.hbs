    <!-- Breadcrumb Form Section Begin -->

 <!-- Shopping Cart Section Begin -->
    <section class="shopping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="cart-table">
                        <table>
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th class="p-name">Product Name</th>
                                    <th>Price</th>
                                    <th>Quantity</th>
                                    {{!-- <th>Total</th> --}}
                                    <th><i class="ti-close"></i></th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each orders}}
                                        {{#product}}
                                <tr>
                                    {{!-- {{#each this.product}} --}}
                                    <td class="cart-pic first-row"><img src="/images/{{product._id}}.jpg" alt=""></td>
                                    <td class="cart-title first-row">
                                        
                                        <h5>{{product.product_name}}</h5>
                                    </td>
                                    <td class="p-price first-row">{{product.product_price}}</td>
                                    {{!-- {{/each}} --}}
                                    <td class="qua-col first-row">
                                        
                                                                                <h5>{{quantity}}</h5>

                                        
                                    </td>
                                    
                                    {{!-- {{#each this.product}} --}}
                                    {{!-- <td class="total-price first-row">{{this.product.product_price}}</td> --}}
                                    <td>
                                        <button type="button" class="btn btn-success">Order Placed</button>
                                    </td>
                                    {{!-- <td class="close-td first-row"><i class="ti-close"></i></td> --}}
                                    {{!-- {{/each}} --}}
                                </tr>
                                        {{/product}}
                                {{/each}}
                                
                            </tbody>
                        </table>
                    </div>
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="cart-buttons">
                                <a href="#" class="primary-btn continue-shop">Continue shopping</a>
                            </div>
                            
                        </div>
                        <div class="col-lg-4 offset-lg-4">
                            <div class="proceed-checkout">
                                <ul>
                                    <li class="subtotal">Subtotal <span class="total">{{order.status}}</span></li>
                                    <li class="cart-total">Total <span class="total">{{totalAmount}}</span></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>